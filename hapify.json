{
  "version": "1",
  "validatorPath": "hapify/validator.js",
  "name": "HapiJS",
  "description": "API built with HapiJS v17",
  "logo": "https://files-tractr.s3.ca-central-1.amazonaws.com/assets/hapi.svg",
  "project": "hapify-models.json",
  "defaultFields": [
    {
      "name": "_id",
      "type": "string",
      "subtype": null,
      "reference": null,
      "primary": true,
      "unique": false,
      "label": false,
      "nullable": false,
      "multiple": false,
      "embedded": false,
      "searchable": false,
      "sortable": false,
      "hidden": false,
      "internal": true,
      "restricted": false,
      "ownership": false
    },
    {
      "name": "created at",
      "type": "datetime",
      "subtype": null,
      "reference": null,
      "primary": false,
      "unique": false,
      "label": false,
      "nullable": false,
      "multiple": false,
      "embedded": false,
      "searchable": false,
      "sortable": true,
      "hidden": false,
      "internal": true,
      "restricted": false,
      "ownership": false
    }
  ],
  "templates": [
    {
      "path": "cmd/setup/indexes.json",
      "engine": "js",
      "input": "all"
    },
    {
      "path": "models.json",
      "engine": "js",
      "input": "all"
    },
    {
      "path": "MODELS.md",
      "engine": "js",
      "input": "all"
    },
    {
      "path": "routes/{kebab}/create.js",
      "engine": "hpf",
      "input": "one"
    },
    {
      "path": "routes/{kebab}/delete.js",
      "engine": "hpf",
      "input": "one"
    },
    {
      "path": "routes/{kebab}/inc.js",
      "engine": "hpf",
      "input": "one"
    },
    {
      "path": "routes/{kebab}/index.js",
      "engine": "hpf",
      "input": "one"
    },
    {
      "path": "routes/{kebab}/read.js",
      "engine": "hpf",
      "input": "one"
    },
    {
      "path": "routes/{kebab}/search.js",
      "engine": "hpf",
      "input": "one"
    },
    {
      "path": "routes/{kebab}/update.js",
      "engine": "hpf",
      "input": "one"
    },
    {
      "path": "routes/index.js",
      "engine": "hpf",
      "input": "all"
    }
  ]
}
